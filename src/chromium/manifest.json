{
  "chrome_url_overrides": {
    "newtab": "iframe.html"
  },
  "manifest_version": 2,
  "description": "Raise money for charity with every browser tab you open!",
  "icons": {
    "16": "./img/logo16.png",
    "32": "./img/logo32.png",
    "48": "./img/logo48.png",
    "128": "./img/logo128.png"
  },
  "name": "Tab for a Cause",
  "background": {
    "scripts": ["ext-background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "5.19"
}
