{
  "name": "Tab for a Cause",
  "description": "Raise money for charity with every browser tab you open!",
  "version": "5.11",
  "icons": {
    "16": "./img/logo16.png",
    "32": "./img/logo32.png",
    "48": "./img/logo48.png",
    "128": "./img/logo128.png"
  },
  "background": {
    "scripts": ["ext-background.js"],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "iframe.html"
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "strict_min_version": "56.0a1"
    }
  }
}
